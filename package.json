{
  "name": "kepler-space",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3000",
    "preview": "vite preview",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix",
    "lint:no-warning": "yarn lint --max-warnings=0",
    "format": "prettier --write src/",
    "verify_pull_request": "run-p type-check lint:no-warning build -l"
  },
  "dependencies": {
    "i18next": "^23.15.2",
    "phaser": "^3.85.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.3",
    "vite-plugin-pwa": "^0.20.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.8"
  }
}
